{
    "name": "filipac/withings-sdk",
    "description": "Laravel package for Withings API integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Filip CÃ¡rdenas",
            "email": "filip@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Filipac\\Withings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Filipac\\Withings\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Filipac\\Withings\\WithingsServiceProvider"
            ],
            "aliases": {
                "Withings": "Filipac\\Withings\\Facades\\Withings"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}